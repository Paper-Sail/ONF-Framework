{
    "id": "/arte/veryveryshort/prod/papersail-php",
    "cpus": 0.1,
    "mem": 512,
    "container": {
      "type": "MESOS",
      "volumes": [
      ],
      "docker": {}
    },
    "env": {
    },
    "healthChecks": [
      {
        "path": "/ping.html",
        "protocol": "HTTP",
        "port": 80,
        "gracePeriodSeconds": 45,
        "intervalSeconds": 10,
        "timeoutSeconds": 10,
        "maxConsecutiveFailures": 3
      }
    ],
    "labels": {
      "HAPROXY_GROUP": "external",
      "HAPROXY_0_REDIRECT_TO_HTTPS": true,
      "HAPROXY_0_VHOST": "papersail.veryveryshort.com, papersail.trestrescourt.com, papersail.sehrsehrkurz.com"
    },
    "portDefinitions": [],
    "ipAddress": {
      "discovery": {
        "ports": [
          {
            "number": 80,
            "name": "http",
            "protocol": "tcp",
            "labels": {}
          }
        ]
      },
      "networkName": "vlan"
    }
  }